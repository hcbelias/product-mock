{
  "name": "product-mock",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "debug": "set DEBUG=product-mock:* && node ./bin/www",
    "debug-win": "node inspect ./bin/www",
    "debug-server": "set NODE_ENV=development && set DEBUG=myapp:* && npm run debug",
    "port-debug": "netstat -ano | findstr :9229",
    "kill-debug": "taskkill /F /PID"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.17.3",
    "faker": "^4.1.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1"
  }
}